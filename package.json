{
  "name": "imock",
  "displayName": "Imock",
  "description": "前端 mock 后端接口，支持开启和关闭",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.startMockServer",
    "onCommand:extension.stopMockServer",
    "onCommand:extension.showConfigDialog",
    "onView:imockTreeView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.startMockServer",
        "title": "开启 Mock 服务"
      },
      {
        "command": "extension.stopMockServer",
        "title": "关闭 Mock 服务"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "imockViewContainer",
          "title": "IMock",
          "icon": "media/imock.svg" 
        }
      ]
    },
    "views": {
      "imockViewContainer": [
        {
          "id": "imockTreeView",
          "name": "Mock 服务列表",
          "when": "imockExtensionEnabled"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "test": "vscode-test",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "element-plus": "^2.3.7",
    "vue": "^3.2.47"
  }
}